{
  "name": "tt-tool",
  "version": "1.0.0",
  "main": "index.js",
  "author": "",
  "private": true,
  "scripts": {
    "clean-mac": "rm -rf dist && rm -rf deploy/darwin/build",
    "clean-linux": "rm -rf dist && rm -rf deploy/linux/build",
    "clean-win": "rd /s /q dist deploy\\win32\\build",
    "init-packer": "npx nodegui-packer --init tt-tool",
    "pack": "webpack --mode production && npx nodegui-packer --pack ./dist",
    "build": "webpack --mode production",
    "start": "webpack && qode ./dist/index.js",
    "debug": "webpack && qode --inspect ./dist/index.js"
  },
  "dependencies": {
    "@nodegui/nodegui": "^0.57.3",
    "adm-zip": "^0.5.1",
    "cli-progress": "^3.12.0",
    "deepl-node": "^1.10.2",
    "iso-639-1": "^2.1.15",
    "original-fs": "^1.2.0",
    "subtitle": "^4.2.1",
    "wget-improved": "^3.4.0"
  },
  "devDependencies": {
    "@nodegui/packer": "^1.5.0",
    "@types/adm-zip": "^0.5.1",
    "@types/cli-progress": "^3.11.0",
    "@types/node": "^18.16.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "file-loader": "^6.2.0",
    "native-addon-loader": "^2.0.1",
    "ts-loader": "^9.4.4",
    "typescript": "^4.9.5",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4"
